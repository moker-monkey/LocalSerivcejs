{
  "name": "localservicejs",
  "version": "3.0.0",
  "description": "用于模仿后端服务，代码来源于mockjs的功能精简，只保留了服务拦截功能，并且在此基础上进行了可异步阻塞的改良，改变了一些方法和变量的命名，使它更贴近使用场景。",
  "main": "./dist/LocalService.cjs",
  "module": "./dist/LocalService.mjs",
  "browser": "./dist/LocalService.js",
  "types": "./src/index.d.ts",
  "keywords": [
    "local",
    "service",
    "js",
    "local service"
  ],
  "scripts": {
    "test": "mocha",
    "build": "cross-env BUILD_MODEL=commonjs webpack -c webpack.config.js",
    "build:rollup": "rollup -c rollup.config.js"
  },
  "author": "moker-monker",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "axios": "^0.26.1",
    "babel-loader": "^8.2.5",
    "core-js": "2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.0.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.18.2",
    "@babel/runtime": "^7.18.3",
    "@babel/runtime-corejs3": "^7.18.3",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-eslint": "^8.0.2",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "mocha": "^10.0.0",
    "rollup-plugin-json": "^4.0.0"
  }
}
